fireOutSound = _SndOpen("assets/fireout.ogg")
glassSound = _SndOpen("assets/glass.ogg")
particlesSound = _SndOpen("assets/particles.ogg")

Const FIRE = 1
Const SMOKE = 2
Const SPARK = 3
Const GRAVITATOR = 4
Const GRAVITATORCELL = 5
